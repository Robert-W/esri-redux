{
  "name": "esri-redux",
  "version": "1.0.0",
  "description": "Simple boilerplate demonstrating how to setup a project using React, Redux, Flow, and the Esri JavaScript API.",
  "main": "scripts/start.js",
  "scripts": {
    "start": "node scripts/start",
    "dist": "rm -rf dist;node scripts/build;node scripts/prerender;",
    "test": "jest --coverage --verbose;eslint src/js/**/*.js;"
  },
  "keywords": [
    "hot",
    "esri",
    "react",
    "redux",
    "Caddy",
    "https",
    "http2",
    "es6",
    "flux",
    "webpack",
    "hot-reload",
    "javascript",
    "esri-webpack",
    "hot-module-replacement"
  ],
  "author": "RobertW",
  "license": "MIT",
  "jest": {
    "moduleNameMapper": {
      "^js(.*)$": "<rootDir>/src/js$1",
      "^dojo(.*)$": "<rootDir>/__mocks__/arcgis.js",
      "esri/request": "<rootDir>/__mocks__/MapView.js",
      "esri/views/MapView": "<rootDir>/__mocks__/MapView.js",
      "esri/Map": "<rootDir>/__mocks__/Map.js",
      "^dijit(.*)$": "<rootDir>/__mocks__/arcgis.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/style.js",
      "\\.(jpe?g|png|gif|svg)$": "<rootDir>/__mocks__/static.js"
    }
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "redux": "^3.6.0",
    "webpack": "^2.2.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.3",
    "babel-core": "^6.23.1",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.3.1",
    "babel-plugin-transform-es2015-modules-amd": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-runtime": "^6.23.0",
    "cheerio": "^0.20.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.13.1",
    "eslint-plugin-react": "^5.2.2",
    "enzyme": "^2.7.1",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "git+https://github.com/webpack/extract-text-webpack-plugin.git#v2.0.0-rc.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^2.0.0",
    "jest": "^19.0.2",
    "node-sass": "^3.13.1",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "react-test-renderer": "^15.4.2",
    "requirejs": "^2.3.2",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.17.0"
  }
}
