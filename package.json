{
  "name": "esri-redux",
  "version": "1.0.0",
  "description": "Simple boilerplate demonstrating how to setup a project using React, Redux, Flow, and the Esri JavaScript API.",
  "main": "scripts/start.js",
  "scripts": {
    "start": "node scripts/start",
    "dist": "rm -rf dist;node scripts/build;node scripts/prerender;",
    "lint": "eslint src/js/**/*.js",
    "test": "jest"
  },
  "keywords": [
    "hot",
    "esri",
    "react",
    "redux",
    "Caddy",
    "https",
    "http2",
    "es6",
    "flux",
    "webpack",
    "hot-reload",
    "javascript",
    "esri-webpack",
    "hot-module-replacement"
  ],
  "author": "Blue Raster",
  "license": "MIT",
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "coverageReporters": [
      "text"
    ],
    "moduleNameMapper": {
      "esri/Map": "<rootDir>/__mocks__/function.js",
      "esri/views/MapView": "<rootDir>/__mocks__/MapView.js",
      "^js(.*)$": "<rootDir>/src/js$1",
      "^dojo(.*)$": "<rootDir>/__mocks__/object.js",
      "^esri(.*)$": "<rootDir>/__mocks__/object.js",
      "^dijit(.*)$": "<rootDir>/__mocks__/object.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/object.js",
      "\\.(jpe?g|png|gif|svg)$": "<rootDir>/__mocks__/object.js"
    }
  },
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "redux": "^3.7.2",
    "webpack": "^3.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-es2015-modules-amd": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "cheerio": "^1.0.0-rc.2",
    "css-loader": "^0.28.7",
    "enzyme": "^2.9.1",
    "eslint": "^4.7.2",
    "eslint-plugin-react": "^7.4.0",
    "express": "^4.15.5",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.4.2",
    "jest": "^21.1.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.6.0",
    "react-hot-loader": "^1.3.1",
    "react-test-renderer": "^15.6.1",
    "requirejs": "^2.3.5",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1"
  }
}
